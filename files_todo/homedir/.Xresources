! Hide/disable scrollbar
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt

! Increase scrollback buURxvt*saveLines: 16384ffer size
URxvt*saveLines: 16384

! Font config
URxvt*font:           xft:Inconsolata:size=8
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint:   false
Xft.lcdfilter:  lcddefault

*.foreground:  #cccccc
*.background:  #1e1e1e
*.cursorColor: #ffffff
